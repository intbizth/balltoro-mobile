<Alloy>
	<Window id="main" module="xp.ui" role="leftWindow">
		<Widget id="navbarView" src="com.intbizth.alloy.navbar" />
		<View id="content">
			<ListView id="list">
				<Templates>
					<ItemTemplate name="inAct" class="itemTemplate">
						<Label bindId="title" class="templateInActTitle" />
						<View class="templateInActBall" />
						<View class="templateInActLine" />
					</ItemTemplate>
					<ItemTemplate name="act" class="itemTemplate">
						<Label bindId="title" class="templateActTitle" />
						<View class="templateActBall" />
						<View class="templateActLine" />
					</ItemTemplate>
				</Templates>
				<ListSection id="section" />
			</ListView>
		</View>
	</Window>
</Alloy>
