<Alloy>
	<Window id="main" module="xp.ui" role="leftWindow">
		<View id="content">
			<Widget id="navbarView" src="com.intbizth.alloy.navbar"></Widget>
			<ListView id="list">
				<Templates>
					<ItemTemplate name="inAct" class="itemTemplate">
						<Label bindId="title" class="templateInActTitle" />
						<View class="templateInActBall"></View>
						<View class="templateInActLine"></View>
					</ItemTemplate>
					<ItemTemplate name="act" class="itemTemplate">
						<Label bindId="title" class="templateActTitle" />
						<View class="templateActBall"></View>
						<View class="templateActLine"></View>
					</ItemTemplate>
				</Templates>
				<ListSection id="section"></ListSection>
			</ListView>
		</View>
	</Window>
</Alloy>
