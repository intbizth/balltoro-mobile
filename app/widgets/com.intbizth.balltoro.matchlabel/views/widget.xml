<Alloy>
    <Collection src="matchelabel" />
    <View id= "main">
        <ListView id="list" onPull="pull" onPullend="pullend" onMarker="marker" onItemclick="itemclick">
            <Templates>
                <ItemTemplate name="nodata" class="nodata_row">
