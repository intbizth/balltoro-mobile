                            </View>
                            <View class="gamelivehalftime_rightIconView">
                                <ImageView class="gamelivehalftime_rightIcon" bindId="rightIcon" />
                            </View>
                        </View>
                    </View>
                </ItemTemplate>
                <ItemTemplate name="section" class="section_row">
                    <View bindId="view">
                        <View class="section_iconView">
                            <ImageView class="section_icon" bindId="icon" />
                        </View>
                        <View class="section_titleLabelView">
                            <Label class="section_titleLabel" bindId="title" />
                        </View>
                    </View>
                </ItemTemplate>
            </Templates>
            <PullView>
                <View id="pullView">
                    <ActivityIndicator id="activityIndicator" />
                </View>
            </PullView>
            <ListSection id="section" dataCollection="matchelabel">
                <ListItem class="listItem" dataId="{id}" template="{template}" icon:image="{icon}" title:text="{title}" leftView:backgroundColor="{leftViewBackgroundColor}" leftView:backgroundColorInAct="{leftViewBackgroundColorInAct}" leftView:backgroundColorAct="{leftViewBackgroundColorAct}" leftIcon:image="{leftIcon}" leftLabel:text="{leftLabel}" centerView:backgroundColor="{centerViewBackgroundColor}" centerView:backgroundColorInAct="{centerViewBackgroundColorInAct}" centerView:backgroundColorAct="{centerViewBackgroundColorAct}" rightView:backgroundColor="{rightViewBackgroundColor}" rightView:backgroundColorInAct="{rightViewBackgroundColorInAct}" rightView:backgroundColorAct="{rightViewBackgroundColorAct}" rightIcon:image="{rightIcon}" rightLabel:text="{rightLabel}" scoreLabel:text="{scoreLabel}" startTimeLabel:text="{startTimeLabel}" startDateLabel:text="{startDateLabel}" />
            </ListSection>
        </ListView>
    </View>
</Alloy>