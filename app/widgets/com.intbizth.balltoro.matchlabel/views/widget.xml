                                <Label class="before_startTimeLabel" bindId="startTimeLabel" />
                                <Label class="before_startDateLabel" bindId="startDateLabel" />
                            </View>
                        </View>
                        <View class="before_rightView" bindId="rightView">
                            <View class="before_rightLabelView">
                                <Label class="before_rightLabel" bindId="rightLabel" />
                            </View>
                            <View class="before_rightIconView">
                                <ImageView class="before_rightIcon" bindId="rightIcon" />
                            </View>
                        </View>
                    </View>
                </ItemTemplate>
                <ItemTemplate name="gameafter" class="gameafter_row">
                    <View bindId="view">
